{
  "name": "nl-rdo-dusi-portal-web-frontend",
  "version": "2.2.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build-only:portal": "VITE_PROJECT=portal vite build",
    "build-only:assessment": "VITE_PROJECT=assessment vite build",
    "build:portal": "run-p type-check build-only:portal",
    "build:assessment": "run-p type-check build-only:assessment",
    "dev:portal": "VITE_PROJECT=portal vite",
    "dev:assessment": "VITE_PROJECT=assessment vite",
    "format": "prettier --write src '!src/public/**/*.{js,css}'",
    "lint": "oxlint -c ./eslintrc.json --ignore-path .eslintignore -D all",
    "lint-fix": "oxlint -c ./eslintrc.json --fix --ignore-path .eslintignore -D all",
    "prepare": "husky",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "npm run lint-fix && npm run format",
    "*.{vue}": "npm run type-check"
  },
  "dependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@form-validation/validator-iban": "^2.4.0",
    "@headlessui/vue": "^1.7.19",
    "@jsonforms/core": "^3.2.1",
    "@jsonforms/vue": "^3.2.1",
    "@jsonforms/vue-vanilla": "^3.2.1",
    "@minvws/nl-rdo-rijksoverheid-ui-theme": "^0.0.18",
    "@types/js-cookie": "^3.0.5",
    "@vueuse/core": "^10.9.0",
    "@vueuse/head": "^2.0.0",
    "ajv": "^8.17.1",
    "dotenv": "^16.4.5",
    "js-cookie": "^3.0.5",
    "libsodium-wrappers": "^0.7.13",
    "lodash.merge": "^4.6.2",
    "pinia": "^2.2.2",
    "sass": "^1.75.0",
    "uuid": "^9.0.1",
    "vue": "~3.4.37",
    "vue-i18n": "^9.14.2",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@optimize-lodash/rollup-plugin": "^4.0.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.5",
    "@types/libsodium-wrappers": "^0.7.12",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^20.12.4",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.3.0",
    "npm-run-all": "^4.1.5",
    "oxlint": "^0.2.15",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "5.4.5",
    "vite": "^5.4.7",
    "vite-bundle-analyzer": "^0.9.4",
    "vite-plugin-html-sort-tags": "^0.1.0",
    "vite-plugin-preload": "^0.4.0",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.6"
  }
}
